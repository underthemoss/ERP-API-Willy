{
  "name": "es-erp-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "commonjs",
  "graphql": {
    "schema": "./src/graphql/schema/generated/schema.graphql",
    "documents": "./src/test/**/*.{graphql,js,ts,jsx,tsx}"
  },
  "scripts": {
    "prebuild": "rm -rf ./build && npm run codegen",
    "build": "tsc",
    "migrate-mongo": "migrate-mongo up -f './src/mongo/migrate-mongo-config.js'",
    "migrate-spicedb": "node -r ts-node/register ./node_modules/migrate-mongo/bin/migrate-mongo.js up -f './spicedb/migrate-spicedb-config.js'",
    "prestart": "npm run migrate-mongo && npm run spicedb:write-schema-authed && npm run migrate-spicedb && node ./ksql/scripts/migrate.js up",
    "start": "node build/index.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier:check": "prettier ./src/ --check",
    "prettier:fix": "prettier ./src/ --write",
    "//": "local development scripts",
    "codegen": "graphql-codegen --config codegen.yml",
    "test": "npm run generate:schema && npm run codegen && DOCKER_CONFIG=/tmp PRETTY_LOGS=true LEVEL=dev npx jest --reports=text --detectOpenHandles --silent=false",
    "test:parallel": "npm run generate:schema && npm run codegen && DOCKER_CONFIG=/tmp PRETTY_LOGS=true LEVEL=dev npx jest --maxWorkers=80%  --silent",
    "test:debug": "DOCKER_CONFIG=/tmp DEBUG_E2E_TESTS=true npm run test",
    "test:report": "open coverage/index.html",
    "with:local": "dotenv -e ./env/.env.local --",
    "with:stage": "dotenv -e ./env/.env.stage --",
    "migrate:create": "npm run with:local -- migrate-mongo create -f './src/mongo/migrate-mongo-config.js'",
    "migrate:status": "npm run with:local -- migrate-mongo status -f './src/mongo/migrate-mongo-config.js'",
    "migrate:up": "npm run with:local -- migrate-mongo up -f './src/mongo/migrate-mongo-config.js'",
    "migrate:down": "npm run with:local -- migrate-mongo down -f './src/mongo/migrate-mongo-config.js'",
    "////": "spicedb migration scripts",
    "migrate-ksql:create": "npm run with:local -- node ./ksql/scripts/migrate.js create",
    "migrate-ksql:status": "npm run with:local -- node ./ksql/scripts/migrate.js status",
    "migrate-ksql:up": "npm run with:local -- node ./ksql/scripts/migrate.js up",
    "postmigrate-ksql:up": "node ./ksql/scripts/post-migrate.js",
    "ksql:codegen": "node ./ksql/scripts/codegen.js && npm run ksql:generate-test-utils && npm run ksql:types",
    "ksql:generate-test-utils": "ts-node ./ksql/scripts/generate-test-utils.ts",
    "ksql:types": "node ./ksql/lib/type-generator.js",
    "test:ksql": "TESTCONTAINERS_REUSE_ENABLE=true jest --config=ksql/jest.config.js --runInBand",
    "///": "seed data scripts",
    "seed:kafka": "node seed-data/seed-kafka.js",
    "migrate-spicedb:create": "npm run with:local -- node -r ts-node/register ./node_modules/migrate-mongo/bin/migrate-mongo.js create -f './spicedb/migrate-spicedb-config.js'",
    "migrate-spicedb:status": "npm run with:local -- node -r ts-node/register ./node_modules/migrate-mongo/bin/migrate-mongo.js status -f './spicedb/migrate-spicedb-config.js'",
    "migrate-spicedb:up": "npm run with:local -- node -r ts-node/register ./node_modules/migrate-mongo/bin/migrate-mongo.js up -f './spicedb/migrate-spicedb-config.js'",
    "migrate-spicedb:down": "npm run with:local -- node -r ts-node/register ./node_modules/migrate-mongo/bin/migrate-mongo.js down -f './spicedb/migrate-spicedb-config.js'",
    "start:local": "docker compose up -d && npm run with:local -- nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.test.ts\" --exec \"ts-node src/index.ts\"",
    "start:stage": "npm run with:stage -- nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.test.ts\" --exec \"ts-node src/index.ts\"",
    "start:cdc": "APP_MODE=cdc npm run with:local -- ts-node src/index.ts",
    "avro-ts-codegen": "npm run with:stage -- ts-node scripts/avro-ts-codegen.ts",
    "generate:schema": "ts-node scripts/generate-schema.ts",
    "dev": "docker compose down && npm run start:local",
    "dev:reset": "docker compose down -v && docker compose up -d",
    "dev:stop": "docker compose down -v",
    "spicedb:codegen": "npm run spicedb:parse-schema && ts-node ./src/lib/spicedb-base-resource/generate-spicedb-types.ts ./spicedb/schema.generated.json ./src/lib/authz/spicedb-generated-types.ts && npm run prettier:fix",
    "test:spicedb:codegen": "npm run test:spicedb:parse-schema && ts-node ./src/lib/spicedb-base-resource/generate-spicedb-types.ts ./src/lib/spicedb-base-resource/tests/schema.generated.json ./src/lib/spicedb-base-resource/tests/generated.ts && npm run prettier:fix",
    "test:spicedb:parse-schema": "ts-node ./src/lib/spicedb-base-resource/spicedb-parse-schema.ts ./src/lib/spicedb-base-resource/tests/schema.zed ./src/lib/spicedb-base-resource/tests/schema.generated.json",
    "spicedb:write-schema": "zed schema write ./spicedb/schema.zed",
    "spicedb:parse-schema": "ts-node ./src/lib/spicedb-base-resource/spicedb-parse-schema.ts ./spicedb/schema.zed ./spicedb/schema.generated.json",
    "spicedb:write-schema-authed": "zed schema write ./spicedb/schema.zed --endpoint ${SPICEDB_ENDPOINT} --token ${SPICEDB_TOKEN}"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.internal.equipmentshare.com/fleet/es-erp-api.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@apollo/client": "^3.13.8",
    "@faker-js/faker": "^9.8.0",
    "@graphql-codegen/cli": "^5.0.6",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-apollo-client-helpers": "^3.0.1",
    "@graphql-codegen/typescript-graphql-request": "^4.4.0",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-inspector/cli": "^5.0.8",
    "@ovotech/avro-ts": "^6.2.0",
    "@testcontainers/kafka": "^11.2.1",
    "@testcontainers/localstack": "^11.7.1",
    "@testcontainers/redis": "^11.5.1",
    "@testcontainers/redpanda": "^11.5.1",
    "@tsconfig/node-lts": "^22.0.1",
    "@tsconfig/recommended": "^1.0.8",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.20",
    "@types/migrate-mongo": "^10.0.5",
    "@types/node": "^22.14.1",
    "@types/node-fetch": "^2.6.12",
    "@types/puppeteer": "^5.4.7",
    "@types/wait-on": "^5.3.4",
    "chalk": "^4.1.2",
    "cross-fetch": "^4.1.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.25.1",
    "graphql-request": "^5.0.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "json-stable-stringify": "^1.3.0",
    "migrate-mongo": "^12.1.3",
    "neostandard": "^0.12.1",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.1.9",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "testcontainers": "^11.2.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "wait-on": "^8.0.3"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@authzed/authzed-node": "^1.5.1",
    "@aws-sdk/client-s3": "^3.826.0",
    "@aws-sdk/s3-request-presigner": "^3.826.0",
    "@confluentinc/kafka-javascript": "^1.7.0",
    "@confluentinc/schemaregistry": "^1.3.1",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@modelcontextprotocol/sdk": "^1.25.0",
    "@opensearch-project/opensearch": "^3.5.1",
    "@pulsecron/pulse": "^1.6.8",
    "@searchkit/api": "^4.14.0",
    "@searchkit/instantsearch-client": "^4.14.1",
    "@sendgrid/mail": "^8.1.6",
    "@sinclair/typebox": "^0.34.33",
    "ai": "^4.3.16",
    "auth0": "^4.29.0",
    "dataloader": "^2.2.3",
    "date-fns": "^4.1.0",
    "disposable-email-domains": "^1.0.62",
    "env-schema": "^6.0.1",
    "fastify": "^5.3.0",
    "fastify-plugin": "^5.0.1",
    "free-email-domains": "^1.2.21",
    "graphql": "^16.10.0",
    "graphql-scalars": "^1.24.2",
    "graphql-subscriptions": "^3.0.0",
    "ioredis": "^5.7.0",
    "jose": "^6.0.10",
    "ksqldb-client": "^1.0.5",
    "lodash": "^4.17.21",
    "mercurius": "^16.1.0",
    "mongodb": "^6.16.0",
    "mongodb-memory-server": "^10.2.1",
    "nexus": "^1.3.0",
    "openai": "^6.8.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "puppeteer": "^24.10.0",
    "searchkit": "^4.15.0",
    "sharp": "^0.34.5",
    "sqids": "^0.3.0",
    "tiny-invariant": "^1.3.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.67"
  }
}
